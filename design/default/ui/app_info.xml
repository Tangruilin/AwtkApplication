<dialog x="c" y="m" w="50%" h="50%" highlight="default(alpha=100)" name="new">
  <view w="100%" h="27" style:bg_color="gray">
    <label name="title" x="15" y="0" w="-15" h="100%" style:text_color="black" style:text_align_h="left" tr_text="信息"/>
    <button name="close" x="r:0" y="0" w="27" h="100%" style:border_color="#00000000" style:bg_color="#00000000" style:over:border_color="#00000000" style:over:bg_color="red" style:pressed:border_color="#00000000" style:pressed:bg_color="red" text="X"/>
  </view>
  <view name="separator" x="0" y="b:65" w="100%" h="1" style:normal:bg_color="gray"/>
  <button name="cancel" x="r:15" y="b:21" w="60" h="27" tr_text="Cancel"/>
  <button name="ok" x="r:90" y="b:21" w="60" h="27" tr_text="Ok"/>
  <rich_text name="rich_text" x="24" y="50" w="361" h="100" text=" Info:\n\t\t          company:\n            version:\n            other:"/>
</dialog>